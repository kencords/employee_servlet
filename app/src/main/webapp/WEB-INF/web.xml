<?xml version="1.0" encoding="UTF-8"?>
<web-app
   xmlns="http://xmlns.jcp.org/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
   metadata-complete="false"
   version="3.1">
 
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>ecc.cords.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RoleServlet</servlet-name>
    <servlet-class>ecc.cords.RoleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmployeeFormServlet</servlet-name>
    <servlet-class>ecc.cords.EmployeeFormServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditEmployeeServlet</servlet-name>
    <servlet-class>ecc.cords.EditEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmployeeProfile</servlet-name>
    <servlet-class>ecc.cords.ProfileServlet</servlet-class>
  </servlet>
 
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RoleServlet</servlet-name>
    <url-pattern>/roles</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmployeeFormServlet</servlet-name>
    <url-pattern>/employeeForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditEmployeeServlet</servlet-name>
    <url-pattern>/editEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmployeeProfile</servlet-name>
    <url-pattern>/employeeProfile</url-pattern>
  </servlet-mapping>

</web-app>